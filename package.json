{
  "name": "oamerge",
  "version": "0.0.0-prerelease",
  "description": "Merge and share OpenAPI endpoints.",
  "bin": "cli.js",
  "type": "module",
  "module": "dist/oamerge.js",
  "main": "dist/oamerge.cjs",
  "exports": {
    ".": {
      "import": "./dist/oamerge.js",
      "require": "./dist/oamerge.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "changeset": "changeset",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint 'src/**/*.js' 'rollup.config.js' 'cli.js'",
    "test:unit": "uvu src '.test.js$'",
    "build": "rollup -c",
    "release": "npm run build && changeset publish"
  },
  "files": [
    "dist",
    "licensing",
    "bin.js",
    "CHANGELOG.md",
    "README.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oamerge/oamerge.git"
  },
  "keywords": [
    "openapi",
    "glob",
    "generator",
    "oamerge",
    "merge"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/oamerge/oamerge/issues"
  },
  "homepage": "https://oamerge.com",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@rollup/plugin-node-resolve": "14.1.0",
    "@rollup/plugin-replace": "4.0.0",
    "eslint": "8.22.0",
    "mri": "1.2.0",
    "rollup": "2.79.0",
    "sade": "1.8.1",
    "uvu": "0.5.6"
  }
}
