{
  "name": "oamerge",
  "version": "0.0.0-prerelease",
  "description": "Merge and share OpenAPI endpoints.",
  "bin": "bin.js",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "eslint '**/*.js' --fix",
    "prepublishOnly": "npm run build",
    "build": "rollup -c"
  },
  "files": [
    "dist",
    "bin.js",
    "CHANGELOG.md",
    "README.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oamerge/oamerge.git"
  },
  "keywords": [
    "openapi",
    "glob",
    "generator",
    "oamerge",
    "merge"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/oamerge/oamerge/issues"
  },
  "homepage": "https://oamerge.com",
  "devDependencies": {
    "eslint": "8.22.0",
    "rollup": "^2.79.0"
  }
}
